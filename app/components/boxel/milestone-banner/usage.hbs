<Freestyle::Usage @slug="milestone-banner">
  <:example>
    <Boxel::MilestoneBanner @title={{this.title}} />
  </:example>
  <:api as |Args|>
    <Args.String
      @name="title"
      @value={{this.title}}
      @description="The milestone title"
      @onInput={{fn (mut this.title)}}
    />
  </:api>
</Freestyle::Usage>
