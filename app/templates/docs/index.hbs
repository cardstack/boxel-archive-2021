<FreestyleGuide
  @title='Boxel Components'
  @subtitle='Living Component Documentation'
>
  <FreestyleSection @name="Components" as |Section|>
    {{#each this.usageComponents as |componentName|}}
      <Section.subsection @name={{format-component-name componentName}}>
        {{component (concat componentName '/usage')}}
      </Section.subsection>
    {{/each}}
  </FreestyleSection>
</FreestyleGuide>
