<Freestyle::Usage @name="CardPay::WorkflowThread">
  <:example>
    <CardPay::WorkflowThread
      @title={{this.title}}
      @milestones={{this.milestones}}
      @workflowBot={{this.workflowBot}}
      @participants={{array this.accountHolder}}
      @orgParticipants={{array this.workflowBot}}
      @addParticipant={{@addParticipant}}
    />
  </:example>
</Freestyle::Usage>
