<Boxel::OrgHeader
  @title={{@title}}
  @logoURL={{@logoURL}}
  class="media-registry__header media-registry__header--{{@id}}"
>
  <input class="media-registry__search-bar" type="search" placeholder="Search" />
  <nav class="media-registry__nav">
    <ul>
      <li>
        <LinkTo @route="media-registry.index" @model={{@id}}>
          {{#if @musicalWorkOnly}}
            Musical Works
          {{else}}
            Masters
          {{/if}}
        </LinkTo>
      </li>
      {{#if @org.user}}
        <li><LinkTo @route="media-registry.agreements" @model="TA-38185847898">Agreements</LinkTo></li>
      {{else}}
        <li><LinkTo @route="media-registry.discrepancies.index">Discrepancies</LinkTo></li>
      {{/if}}
    </ul>
  </nav>
</Boxel::OrgHeader>