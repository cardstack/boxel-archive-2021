<Boxel::Dashboard
  @darkTheme={{true}}
  @displayLeftEdge={{true}}
>
  <:body>
    <section>
      <h2>Join an Org</h2>
      <ul>
        {{#each @model.orgs as |org|}}
          <li>
            {{#if org.url}}
              <LinkTo @route={{org.url}}>
                {{org.title}}
              </LinkTo>
            {{else}}
              {{org.title}}
            {{/if}}
          </li>
        {{/each}}
      </ul>
    </section>
  </:body>

</Boxel::Dashboard>